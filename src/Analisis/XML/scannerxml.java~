            { addLexema("cadena",yytext(), yyline, yychar);
                return new Symbol(sym.cadena,yychar, yyline, yytext().substring(1,yytext().length()-1));
            } 
            // fall through
          case 128: break;
          case 45: 
            { addLexema("reservada", yytext(), yyline, yychar);            
